<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yking.usercenter">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name="com.kotlin.base.common.BaseApplication"
        android:theme="@style/AppTheme">
        <activity android:name="com.kotlin.usercenter.ui.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kotlin.usercenter.ui.activity.RegisterActivity"/>
        <activity android:name="com.kotlin.usercenter.ui.activity.ForgetPwdActivity"/>
    </application>

</manifest>